let exp(x) = e^x

let length(v) = (x(v)^2 + y(v)^2)^0.5

let rotvec(v, r) = {
    let s = sin(r)
    let c = cos(r)

    vec(x(v) * c - y(v) * s, x(v) * s + y(v) * c)
}

let dot(x, y) = x(x) * x(y) + y(x) * y(y)

let ncr(n, r) = n! / ((n-r)! * r!)

let rep(a, x, v) = ins(rm(a, x), x, v)

let abs(x) = x < 0 => -x | x